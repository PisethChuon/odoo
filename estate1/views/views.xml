<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="estate1.list">
      <field name="name">estate1 list</field>
      <field name="model">estate1.estate1</field>
      <field name="arch" type="xml">
        <list>
          <field name="sale_person_id"/>
          <field name="value"/>
          <field name="value2"/>
          
        </list>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="estate1.action_window">
      <field name="name">estate1 window</field>
      <field name="res_model">estate1.estate1</field>
      <field name="view_mode">list,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="estate1.action_server">
      <field name="name">estate1 server</field>
      <field name="model_id" ref="model_estate1_estate1"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "list,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="estate1" id="estate1.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="estate1.menu_1" parent="estate1.menu_root"/>
    <menuitem name="Menu 2" id="estate1.menu_2" parent="estate1.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="estate1.menu_1_list" parent="estate1.menu_1"
              action="estate1.action_window"/>
    <menuitem name="Server to list" id="estate1" parent="estate1.menu_2"
              action="estate1.action_server"/>

  </data>
</odoo>
